call, file='../../hsr.bmad'

parameter[particle] = #197antiAu-79

mau = mass_of(#197antiAu-79)
gpi = 46.5/anomalous_moment_of(proton)
bgpi = sqrt(gpi*gpi-1)
pau = 79*bgpi*m_proton
eau = sqrt(pau^2+mau^2)
pauq = pau/79
emau = 2.5e-6*mau/pau
dpau = 6e-4
nsaux = 7
nsauy = 6

beginning[e_tot]    = eau

call, file='rhic.bmad'
call, file='ir2.bmad'
call, file='ir4.bmad'
call, file='ir6.bmad'
call, file='ir8n.bmad'
call, file='ir10.bmad'
call, file='ir12.bmad'

use, hsr_hi_n
