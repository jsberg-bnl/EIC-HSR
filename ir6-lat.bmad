per_b1apf: patch

! These are RHIC DX magnets
b1pr: sbend
b2pr: sbend
o_b1pr : overlay={
 b1pr[l]:lcendx/sinc(0.5*angle),
 b1pr[g]:2*sin(0.5*angle)/lcendx,
 b1pr[e1]:0.5*angle,
 b1pr[e2]:0.5*angle},
 var={angle}
o_b2pr : overlay={
 b2pr[l]:lcendx/sinc(0.5*angle),
 b2pr[g]:2*sin(0.5*angle)/lcendx,
 b2pr[e1]:0.5*angle,
 b2pr[e2]:0.5*angle},
 var={angle}

! Use a DX on the IP side of the widdershins crab
h5_dh2: sbend, l=lcendx
o5_dh2c: drift, l=ldxfla*cos(thdx)
pr5_dh2: patch
pt5_dh2: patch
h5_d2: line=(pr5_dh2,pt5_dh2,o5_dh2c,h5_dh2,odxflx)
ov_h5_d2 : overlay={
 h5_dh2[dg] : sin(th2)/lcendx,
 pt5_dh2[x_offset] : lcendx*tan(0.5*th2)+ldxfla*cos(thdx)*tan(th2),
 pr5_dh2[x_pitch] : -th2},
 var = {th2}

oint: drift, l=0.3

