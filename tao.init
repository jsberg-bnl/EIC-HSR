 design_lattice(10) = 'hsr-275-10.bmad'
 design_lattice(11) = 'hsr-275-10.bmad'
 design_lattice(12) = 'hsr-275-10.bmad'
 design_lattice(13) = 'hsr-275-10.bmad'
 design_lattice(14) = 'hsr-275-10.bmad'
 design_lattice(15) = 'hsr-275-10.bmad'
 design_lattice(16) = 'hsr-275-10.bmad'
 design_lattice(17) = 'hsr-275-10.bmad'
 design_lattice(18) = 'hsr-275-10.bmad'
 design_lattice(19) = 'hsr-275-10.bmad'
 design_lattice(20) = 'hsr-275-10.bmad'
 design_lattice(21) = 'hsr-275-10.bmad'
 design_lattice(22) = 'hsr-275-10.bmad'
 design_lattice(23) = 'hsr-275-10.bmad@ir12_41'
 design_lattice(24) = 'hsr-275-10.bmad' 'ir4-ovl.bmad'
 ! Insertions
 design_lattice(2)%slice_lattice = 'yo5_int11_2:ip6w' ! ir6w
 design_lattice(2)%reverse_lattice = t
 design_lattice(3)%slice_lattice = 'ip6d:yi6_int11_2' ! ir6d
 design_lattice(4)%slice_lattice = 'yi7_int9_3:ip8'  ! ir8w
 design_lattice(4)%reverse_lattice = t
 design_lattice(5)%slice_lattice = 'ip8:yo8_int11_2' ! ir8d
 design_lattice(6)%slice_lattice = 'yo9_int11_2:bo10_int11_2'  ! ir10
 design_lattice(7)%slice_lattice = 'bo11_int11_2:bi12_int11_2' ! ir12
 design_lattice(8)%slice_lattice = 'bi1_int11_2:yi2_int11_2'   ! ir2
 design_lattice(9)%slice_lattice = 'yi3_int11_2:yo4_int11_2'   ! ir4
 ! periodic cells
 design_lattice(10)%slice_lattice = 'o10int_pr+1:yi6_int11_2'  ! yi beg
 design_lattice(11)%slice_lattice = 'yi3_int11_2:yi3_int9_3-1' ! yi end
 design_lattice(12)%slice_lattice = 'yi7_int11_2:lmp07y10+1'   ! yi end rev
 design_lattice(12)%reverse_lattice = t
 design_lattice(13)%slice_lattice = 'yo4_int9_3+1:yo4_int11_2' ! yo beg
 design_lattice(14)%slice_lattice = 'yo9_int11_2:yo9_int9_3-1' ! yo end
 design_lattice(15)%slice_lattice = 'yo4_int9_3+1:yo4_int11_2' ! yo beg rev
 design_lattice(15)%reverse_lattice = t
 design_lattice(16)%slice_lattice = 'yo9_int11_2:yo9_int9_3-1' ! yo end rev
 design_lattice(16)%reverse_lattice = t
 design_lattice(17)%slice_lattice = 'bi12_int9_3+1:bi12_int11_2' ! bi beg
 design_lattice(18)%slice_lattice = 'bi1_int11_2:bi1_int9_3-1'   ! bi end
 design_lattice(19)%slice_lattice = 'bo10_int9_3+1:bo10_int11_2' ! bo beg
 design_lattice(20)%slice_lattice = 'bo11_int11_2:bo11_int9_3-1' ! bo end
 ! Other slices of insertions
 design_lattice(21)%slice_lattice = 'yi7_int9_3:yo8_int11_2'   ! ir8 rev
 design_lattice(21)%reverse_lattice = t
 design_lattice(22)%slice_lattice = 'bo11_dh0-1:bi12_dh0+1'    ! inner ir12
 design_lattice(23)%slice_lattice = 'oir12_41:bi12_dh0+2'      ! inner ir12, 41 GeV
 design_lattice(24)%slice_lattice = 'yi3_b1+2:yo4_b1-2'        ! inner ir4
/

